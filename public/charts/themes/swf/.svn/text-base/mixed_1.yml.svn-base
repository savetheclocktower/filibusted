<%=chart :mixed %>

  <%=comp :axis_category %>
    size: 11
    alpha: 70
    bold: true
    color: 000000
    font: arial

  <%=comp :axis_value %>
    steps: 4
    prefix: 
    size: 10
    alpha: 70
    separator: 
    decimals: 0
    color: ffffff
    show_min: true
    suffix: 

  <%=comp :chart_border %>
    top_thickness: 0
    bottom_thickness: 2
    left_thickness: 0
    color: 000000
    right_thickness: 0

  <%=comp :chart_grid_h %>
    alpha: 20
    thickness: 1
    type: dashed
    color: 000044

  <%=comp :chart_pref %>
    fill_shape: true
    point_shape: circle
    line_thickness: 2

  <%=comp :chart_rect %>
    x: 75
    positive_alpha: 90
    y: 50
    height: 200
    positive_color: 777799
    negative_color: 000000
    width: 300

  <%=comp :chart_value %>
    prefix: 
    as_percentage: false
    alpha: 60
    size: 11
    separator: 
    decimals: 0
    color: 0000ff
    position: below
    suffix: 

  <%=comp :draw %> 
    components:         
    - <%=drawing :text %>    
        alpha:     10
        rotation: -90
        size:      35
        x:         -10
        y:         300
        bold:      true
        color:     '000000'
        height:    200
        h_align:   left
        width:     300
        v_align:   top   
        text:      |||||||||||||||||||||||||||||||||
      
    - <%=drawing :text %>
        alpha: 35
        size: 65
        x: 15
        y: -11
        bold: true
        color: 88FF00
        height: 200
        h_align: center
        width: 400
        v_align: top                           
        text: net growth 

  <%=comp :legend_label %>
    size: 11
    alpha: 60
    bullet: circle
    bold: true
    color: 000000
    font: arial

  <%=comp :legend_rect %>
    line_alpha: 0
    x: 90
    fill_alpha: 0
    y: 58
    margin: 5
    line_color: 000000
    height: 40
    fill_color: 000000
    line_thickness: 0
    width: 170     
    
  <%=comp :series_color %>
     colors: eeee88,88eeee,8888ee,ff8800